  <data name="python" xml:space="preserve">
    <value>import sys,importlib,datetime
from urllib import parse
import json

if len(sys.argv)!= 2:
    print(len(sys.argv))
    print("命令行参数长度应为2，传参错误")
    sys.exit()
else:
    JsonStr = parse.unquote(sys.argv[1])
    if (JsonStr[0:2] != '''{"'''):
        file_object = open(JsonStr)
        try:
            JsonStr = file_object.read()
            JsonStr = parse.unquote(JsonStr)
        finally:
            file_object.close()
    JsonArray = json.loads(JsonStr)
	
#以上部分是将传入的变量数据urldecode以后再转化成json变量，请不要修改

    #JsonArray['字符']="新的字符值"
    #JsonArray['数据']=20191010
    #JsonArray['列表'].append('增加一个列表')

#以下是对json变量进行编码转成json数据，返回给调用程序，请不要修改
    JsonArray = json.dumps(JsonArray)
    print(JsonArray)</value>
